/* workflow*/
div#container {
	overflow: hidden;
	width: 600px;
	margin-top: 0px;
	margin-right: auto;
	margin-bottom: 10px;
	margin-left: auto;

}
ul#filter {
	float: left;
	font-size: 14px;
	list-style: none;
	margin-left: 0;
	width: 100%;
	font-family: Verdana, Arial;
	color: #333333;
	margin-top: 10px;
	margin-bottom: 10px;
}
ul#filter li {
	border-right: 1px solid #dedede;
	float: left;
	line-height: 16px;
	margin-right: 10px;
	padding-right: 10px;
}
ul#filter li:last-child {
	border-right: none;
	margin-right: 0;
	padding-right: 0;
}
ul#filter a {
	color: #999;
	text-decoration: none;
}
ul#filter li.current a, ul#filter a:hover {
	text-decoration: underline;
}
ul#filter li.current a {
	color: #333;
	font-weight: bold;
}
ul#portfolio {
	float: left;
	list-style: none;
	margin-left: 0;
	width: 600px;

}
ul#portfolio li {
	float: left;
	width: 140px;
	margin-top: 8px;
	margin-right: 10px;
	margin-bottom: 10px;
}
ul#portfolio a {
	display: block;
	width: 100%;
}
ul#portfolio a:hover {
	text-decoration: none;
}
ul#portfolio img {

	display: block;
	padding-bottom: 5px;
}

