@charset "utf-8";

#menu {
font-family: verdana, sans-serif; 
width:950px; 
position:relative; 
font-size:1.1em;
z-index:999;


}
#menu {list-style-type:none; margin:0; padding:0;}
#menu li {
	float:left;
	padding:0;
	margin:0 1px 0 0;
	position:relative;
	width:150px;
	height:30px;
	color: #FFFFFF;
	z-index:999;
	

}
#menu li dl {position:absolute; top:0px; left:0;padding-bottom:0px;z-index:999;border:1px solid red;}
#menu li a, #menu li a:visited {text-decoration:none;}
#menu li dd {display:none;z-index:999;}
#menu li a:hover {border:0;}
#menu li:hover dd, #menu li a:hover dd {display:block;}
#menu li:hover dl, #menu li a:hover dl {padding-bottom:0px;}
#menu table {border-collapse:collapse; padding:0; margin:-1px; font-size:1em;}
#menu dl {width: 150px; margin: 0; padding: 0; z-index:999;}
#menu dt {margin:0; padding: 5px 5px 5px 20px; font-size: 1.1em; color: #fff;z-index:999;}


#menu .one dt {
	border-right-width: 1px;
	border-right-style: solid;
	border-right-color: #8DCDA9;
	z-index:999;
}
#menu .two dt {
	background-image: url(../images/down-arrow3.gif);
	background-repeat: no-repeat;
	background-position: 100px 6px;
	border-right-width: 1px;
	border-right-style: solid;
	border-right-color: #8DCDA9;
	z-index:999;
}

#menu dd {margin:0; padding:0; color: #fff; font-size: 1em; text-align:left;z-index:999;}
#menu .one dd {border-bottom:1px solid #326570;z-index:999;}
#menu .two dd {
	border-bottom:1px solid #79B1C5;
		line-height:20px;z-index:999;

}

#menu dd.last {border-bottom:1px solid #fff;z-index:999;}
#menu dt a, #menu dt a:visited {
	display:block;
	color:#FFFFFF;
	font-family: Verdana, Arial, "宋体";
	font-size: 12px;
}
#menu dd a, #menu dd a:visited {
	color:#fff;
	text-decoration:none;
	display:block;
	padding:10px 0px 10px 19px;
	width:130px;

}
#menu .one dd a {	color:#fff;}
#menu .two dd a {
	color:#fff;
	background-image: url(/_layouts/CAResources/themeCA/images/menu-trans3.png);
	background-repeat: repeat-x;

}

#menu .one dd a:hover {background: #b2ab9b; color:#345;}
#menu .two dd a:hover {
	background-image: url(/_layouts/CAResources/themeCA/images/menu-trans4.png);
	background-repeat: repeat-x;
	color:#fff;

}

.menu_table {
	width:950px;
	height:29px;
	_background: url(/_layouts/CAResources/themeCA/images/menu-trans.png) no-repeat !important;
filter: progid:DXImageTransform.Microsoft.AlphaImageLoader(enabled=true, sizingMethod=noscale, src="/_layouts/CAResources/themeCA/images/menu-trans.png");
	background:none;
	border-bottom-width: 1px;
	border-bottom-style: solid;
	border-bottom-color: #194756;
	z-index:999;
}